<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:background">#049a95</item>
        <item name="android:actionBarStyle">@style/MyActionBarColor</item>
    </style>

    <style name="MyActionBarColor" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:background">#049a95</item>
        <item name="android:logo">@drawable/ds_icon</item>
    </style>



</resources>
